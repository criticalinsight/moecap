{
    "buildCommand": "npm run build",
    "outputDirectory": ".",
    "installCommand": "npm install",
    "rewrites": [
        {
            "source": "/100",
            "destination": "/100/web/dist/index.html"
        },
        {
            "source": "/100/:path*",
            "destination": "/100/web/dist/:path*"
        }
    ],
    "headers": [
        {
            "source": "/100/assets/:path*",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}